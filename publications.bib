	keywords = {biological models, biological system modeling, biology computing, cellular biophysics, comprehensive model, computational biology, computational modeling, current standard, cytological, education, female, humans, mathematical model, optical wavelength conversion, search, simulation, software, standards, systems biology, systems biology markup language, {WC} modeling standards, whole-cell modeling}
