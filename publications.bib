	url = {http://www.sciencedirect.com/science/article/pii/S0167642316000125},
	doi = {10.1016/j.scico.2016.01.002},
	series = {Formal Methods for Industrial Critical Systems ({FMICS}’2014)},
	abstract = {A fault-tolerant routing algorithm in Network-on-Chip ({NoC}) architectures provides adaptivity for on-chip communications. Adding fault-tolerance adaptivity to a routing algorithm increases its design complexity and makes it prone to deadlock and other problems if improperly implemented. Formal verification techniques are needed to check the correctness of the design. This paper describes the discovery of a potential livelock problem through formal analysis on an extension of the link-fault tolerant {NoC} architecture introduced by Wu et al. In the process of eliminating this problem, an improved routing architecture is derived. The improvement simplifies the routing architecture, enabling successful verification using the {CADP} verification toolbox. The routing algorithm is proven to have several desirable properties including deadlock and livelock freedom, and tolerance to a single-link-fault.},
	pages = {24--39},
	journaltitle = {Science of Computer Programming},
	shortjournal = {Science of Computer Programming},
	author = {Zhang, Zhen and Serwe, Wendelin and Wu, Jian and Yoneda, Tomohiro and Zheng, Hao and Myers, Chris},
	urldate = {2020-09-17},
	date = {2016-03-01},
	langid = {english},
	note = {00000
tex.ids: {ImprovedFaulttolerantRouting}\_2016},
	keywords = {fault-tolerant routing, formal methods, model checking, network-on-chip, process calculus}
}

@article{baig_synthetic_2020-1,
	title = {Synthetic biology open language ({SBOL}) version 3.0.0},
	volume = {17},
	issn = {1613-4516},
	url = {https://www.degruyter.com/view/journals/jib/17/2-3/article-20200017.xml},
	doi = {10.1515/jib-2020-0017},
	abstract = {Synthetic biology builds upon genetics, molecular biology, and metabolic engineering by applying engineering principles to the design of biological systems. When designing a synthetic system, synthetic biologists need to exchange information about multiple types of molecules, the intended behavior of the system, and actual experimental measurements. The Synthetic Biology Open Language ({SBOL}) has been developed as a standard to support the specification and exchange of biological design information in synthetic biology, following an open community process involving both wet bench scientists and dry scientific modelers and software developers, across academia, industry, and other institutions. This document describes {SBOL} 3.0.0, which condenses and simplifies previous versions of {SBOL} based on experiences in deployment across a variety of scientific and industrial settings. In particular, {SBOL} 3.0.0, (1) separates sequence features from part/sub-part relationships, (2) renames Component Definition/Component to Component/Sub-Component, (3) merges Component and Module classes, (4) ensures consistency between data model and ontology terms, (5) extends the means to define and reference Sub-Components, (6) refines requirements on object {URIs}, (7) enables graph-based serialization, (8) moves Systems Biology Ontology ({SBO}) for Component types, (9) makes all sequence associations explicit, (10) makes interfaces explicit, (11) generalizes Sequence Constraints into a general structural Constraint class, and (12) expands the set of allowed constraints.},
	number = {2},
	journaltitle = {Journal of Integrative Bioinformatics},
	author = {Baig, Hasan and Fontanarrosa, Pedro and Kulkarni, Vishwesh and {McLaughlin}, James Alastair and Vaidyanathan, Prashant and Bartley, Bryan and Beal, Jacob and Crowther, Matthew and Gorochowski, Thomas E. and Grünberg, Raik and Misirli, Goksel and Scott-Brown, James and Oberortner, Ernst and Wipat, Anil and Myers, Chris J.},
	urldate = {2020-09-16},
	date = {2020-06-25},
	note = {00000
tex.ids: {SyntheticBiologyOpen}\_baig\_2020b
publisher: De Gruyter
section: Journal of Integrative Bioinformatics},
	keywords = {{SBOL}, genetic circuit}
}

